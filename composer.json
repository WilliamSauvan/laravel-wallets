{
    "name": "webqamdev/laravel-wallets",
    "description": "A package to manage Google and Apple Wallet services",
    "type": "library",
    "require": {
        "php": "^8.0",
        "pkpass/pkpass": "^2.3",
        "guzzlehttp/guzzle": "^7.8",
        "google/auth": "^1.40",
        "google/apiclient-services": "^0.358.0",
        "google/apiclient": "^2.16",
        "laravel/framework": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Webqamdev\\LaravelWallets\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webqamdev\\LaravelWallets\\ServiceProvider"
            ],
            "aliases": {
                "GoogleWallet": "Webqamdev\\LaravelWallets\\Facades\\GoogleWallet"
            }
        }
    },
    "require-dev": {
        "laravel/pint": "^1.16"
    }
}
